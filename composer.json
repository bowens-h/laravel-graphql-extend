{
    "name": "bowens-h/laravel-graphql-extend",
    "description": "A extend for Folkloreatelier/laravel-graphql.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "search",
        "database"
    ],
    "authors": [
        {
            "name": "韩文博",
            "email": "hwbrzzl@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "folklore/graphql": "^1.1",
        "doctrine/dbal": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "composer/composer": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "BowensH\\LaravelGraphQLExtend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BowensH\\LaravelGraphQLExtend\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BowensH\\LaravelGraphQLExtend\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
